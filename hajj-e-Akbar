#include <iostream>
using namespace std;

int main(){
	string kata;
	int i = 1;

	do {
        cin>>kata;
        if (kata == "Hajj") {
            cout<<"Case "<<i<<": Hajj-e-Akbar"<<endl;
        } else if (kata == "*") {
            break;
        } else {
            cout<<"Case "<<i<<": Hajj-e-Asghar"<<endl;
        }
        i++;
	} while (kata != "*");

	return 0;
}
