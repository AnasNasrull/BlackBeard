#include <iostream>
#include <stdlib.h>
using namespace std;

int main () {
    string kata;
    int n;

    cin>>n;
    for (int i=0; i<n; i++) {
        cin>>kata;
        for (int j=0; j<kata.length(); j++) {
            if (kata.length()==3 && kata[j]=='o' || kata[j]=='n' || kata[j]=='e') {
                cout<<"1"<<endl;
                break;
            } else if (kata.length()==3 && kata[j]=='t' || kata[j]=='w' || kata[j]=='o') {
                cout<<"2"<<endl;
                break;
            } else if (kata.length()==5){
                cout<<"3"<<endl;
                break;
            }
        }
    }

    return 0;
}
